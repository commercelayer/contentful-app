import{ApiResource as p}from"../resource";class e extends p{async list(t,r){return this.resources.list({type:e.TYPE},t,r)}async retrieve(t,r,i){return this.resources.retrieve({type:e.TYPE,id:t},r,i)}isGeocoder(t){return t.type&&t.type===e.TYPE}relationship(t){return typeof t=="string"?{id:t,type:e.TYPE}:{id:t.id,type:e.TYPE}}type(){return e.TYPE}}e.TYPE="geocoders";export default e;