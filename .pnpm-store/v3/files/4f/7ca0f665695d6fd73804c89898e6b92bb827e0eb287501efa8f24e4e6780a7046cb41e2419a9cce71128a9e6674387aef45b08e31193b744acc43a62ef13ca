import{ApiResource as p}from"../resource";class t extends p{async list(e,r){return this.resources.list({type:t.TYPE},e,r)}async retrieve(e,r,i){return this.resources.retrieve({type:t.TYPE,id:e},r,i)}isRefund(e){return e.type&&e.type===t.TYPE}relationship(e){return typeof e=="string"?{id:e,type:t.TYPE}:{id:e.id,type:t.TYPE}}type(){return t.TYPE}}t.TYPE="refunds";export default t;