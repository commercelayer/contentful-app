import{ApiResource as a}from"../resource";class e extends a{async list(t,r){return this.resources.list({type:e.TYPE},t,r)}async create(t,r,i){return this.resources.create(Object.assign(Object.assign({},t),{type:e.TYPE}),r,i)}async retrieve(t,r,i){return this.resources.retrieve({type:e.TYPE,id:t},r,i)}async update(t,r,i){return this.resources.update(Object.assign(Object.assign({},t),{type:e.TYPE}),r,i)}async delete(t,r){await this.resources.delete({type:e.TYPE,id:t},r)}isAddress(t){return t.type&&t.type===e.TYPE}relationship(t){return typeof t=="string"?{id:t,type:e.TYPE}:{id:t.id,type:e.TYPE}}type(){return e.TYPE}}e.TYPE="addresses";export default e;