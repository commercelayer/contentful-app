import { GetCustomerToken, User } from './salesChannel';
import { GetWebappToken } from './webapp';
import clientCredentials, { ClientCredentials } from './clientCredentials';
import { AuthReturnType, AuthScope, ClientId, ClientSecret } from './typings';
import getRefreshToken from './refreshToken';
declare const authorizeWebapp: import("./webapp").Webapp;
declare const getCustomerToken: GetCustomerToken;
declare const getIntegrationToken: import("./integration").Integration;
declare const getSalesChannelToken: import("./salesChannel").SalesChannel;
declare const getWebappToken: GetWebappToken;
export { AuthReturnType, AuthScope, ClientCredentials, ClientId, ClientSecret, User, authorizeWebapp, clientCredentials, getCustomerToken, getIntegrationToken, getRefreshToken, getSalesChannelToken, getWebappToken, };
declare const CLayerAuth: {
    authorizeWebapp: import("./webapp").Webapp;
    clientCredentials: typeof clientCredentials;
    getCustomerToken: GetCustomerToken;
    getIntegrationToken: import("./integration").Integration;
    getRefreshToken: typeof getRefreshToken;
    getSalesChannelToken: import("./salesChannel").SalesChannel;
    getWebappToken: GetWebappToken;
};
export default CLayerAuth;
