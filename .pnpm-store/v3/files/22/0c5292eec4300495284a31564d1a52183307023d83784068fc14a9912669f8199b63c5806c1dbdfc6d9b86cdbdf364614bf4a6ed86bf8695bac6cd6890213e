import{ApiResource as n}from"../resource";class e extends n{async list(t,s){return this.resources.list({type:e.TYPE},t,s)}async create(t,s,r){return this.resources.create(Object.assign(Object.assign({},t),{type:e.TYPE}),s,r)}async retrieve(t,s,r){return this.resources.retrieve({type:e.TYPE,id:t},s,r)}async delete(t,s){await this.resources.delete({type:e.TYPE,id:t},s)}isOrderCopy(t){return t.type&&t.type===e.TYPE}relationship(t){return typeof t=="string"?{id:t,type:e.TYPE}:{id:t.id,type:e.TYPE}}type(){return e.TYPE}}e.TYPE="order_copies";export default e;