import { AuthReturnType, Endpoint, ClientId, AuthScope, ClientSecret } from './typings';
declare type RefreshTokenParams = {
    clientId: ClientId;
    endpoint: Endpoint;
    refreshToken: string;
    scope?: AuthScope;
    clientSecret?: ClientSecret;
};
export default function refreshToken({ clientId, endpoint, scope, refreshToken, clientSecret, }: RefreshTokenParams): AuthReturnType;
export {};
