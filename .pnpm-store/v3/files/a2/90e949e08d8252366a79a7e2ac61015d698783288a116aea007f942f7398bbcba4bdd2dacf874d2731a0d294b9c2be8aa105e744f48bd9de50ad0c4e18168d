import authenticate from"./authenticate";const webapp=async({clientId,clientSecret,callbackUrl,endpoint,scope,callbackUrlWithCode})=>{const config={clientId,clientSecret,accessTokenUri:`${endpoint}/oauth/token`,authorizationUri:`${endpoint}/oauth/authorize`,redirectUri:callbackUrl};return await authenticate({type:"authorizationCode",config,scope,code:callbackUrlWithCode})};export default webapp;