import{ApiResource as p}from"../resource";class e extends p{async list(t,s){return this.resources.list({type:e.TYPE},t,s)}async create(t,s,c){return this.resources.create(Object.assign(Object.assign({},t),{type:e.TYPE}),s,c)}async retrieve(t,s,c){return this.resources.retrieve({type:e.TYPE,id:t},s,c)}async update(t,s,c){return this.resources.update(Object.assign(Object.assign({},t),{type:e.TYPE}),s,c)}async delete(t,s){await this.resources.delete({type:e.TYPE,id:t},s)}isWireTransfer(t){return t.type&&t.type===e.TYPE}relationship(t){return typeof t=="string"?{id:t,type:e.TYPE}:{id:t.id,type:e.TYPE}}type(){return e.TYPE}}e.TYPE="wire_transfers";export default e;