        if (fn) {
            event.preventDefault();
            fn();
        }
    }, [
        focusedIndex,
        itemsSelector,
        itemsContainerRef,
        keyType
    ]);
    return {
        focusedIndex: focusedIndex,
        handleArrowsKeyDown: handleArrowsKeyDown,
        setFocusedIndex: setFocusedIndex
    };
