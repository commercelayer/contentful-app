#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/alessandro/Documents/code/contentful-app/node_modules/.pnpm/marked@12.0.0/node_modules/marked/bin/node_modules:/Users/alessandro/Documents/code/contentful-app/node_modules/.pnpm/marked@12.0.0/node_modules/marked/node_modules:/Users/alessandro/Documents/code/contentful-app/node_modules/.pnpm/marked@12.0.0/node_modules:/Users/alessandro/Documents/code/contentful-app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/alessandro/Documents/code/contentful-app/node_modules/.pnpm/marked@12.0.0/node_modules/marked/bin/node_modules:/Users/alessandro/Documents/code/contentful-app/node_modules/.pnpm/marked@12.0.0/node_modules/marked/node_modules:/Users/alessandro/Documents/code/contentful-app/node_modules/.pnpm/marked@12.0.0/node_modules:/Users/alessandro/Documents/code/contentful-app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/marked@12.0.0/node_modules/marked/bin/marked.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/marked@12.0.0/node_modules/marked/bin/marked.js" "$@"
fi
